<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
    <namespace>http://superbitsframework/facelets</namespace>
    <tag>
        <tag-name>inputSB</tag-name>
        <source>tags/com/sb/tagInputCampo.xhtml</source>
        <attribute>
            <description>Objeto do tipo campo instanciado</description>
            <name>registro</name>
            <required>true</required>
            <type>com.super_bits.modulosSB.SBCore.InfoCampos.campo.ItfCampoInstanciado</type>
        </attribute>
        <attribute>
            <description></description>
            <name>id</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>

        <attribute>
            <description>indica se o input possui um metodo para ser executando quando alterar</description>
            <name>possuiOnchange</name>
            <required>true</required>
            <type>java.lang.Boolean</type>
        </attribute>

        <attribute>
            <description>Desativar edição</description>
            <name>desabilitarEdicao</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <description>Exibir Modo debug</description>
            <name>modoDebug</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
            <default>#{false}</default>
        </attribute>
        <attribute>
            <description>Exibir Modo debug</description>
            <name>campoLocalizacaoCEP</name>
            <required>false</required>
        </attribute>
    </tag>





    <tag>
        <tag-name>inputSBTestes</tag-name>
        <description><![CDATA[ Componente criado para testar comportamento do metodo onchnage ]]></description>

        <component>
            <component-type>org.super_bits.tags.InputSB</component-type>
            <renderer-type>org.super_bits.tags.InputSBRender</renderer-type>
        </component>

        <attribute>
            <name>metodoOnChange</name>
            <description>metodo onchange</description>
            <required>false</required>
        </attribute>

        <attribute>
            <name>metodoOnChange</name>
            <description>metodo onchange</description>
            <required>false</required>
        </attribute>

        <attribute>
            <name>propriedadeTextoTeste;</name>
            <description>metodo onchange</description>
            <required>false</required>
        </attribute>

    </tag>


</facelet-taglib>
